                  <div class="row">
                    <div class="col-lg-6 col-sm-12 " style="margin-bottom:-200px">
                      <div class="event-content">
                      <img style="width: 100%;border-radius:20px;height:67%" src="/ocop_web/static/src/img/fixmap.jpg"/>
                      </div>
                    </div>
                    <div class="col-lg-6 col-sm-12">
                      <div class="Event-img">
                        <img style="width: 100%;border-radius:20px;height:95%" src="/ocop_web/static/src/img/anh.jpg"/>
                      </div>
                    </div>
